{
  "name": "browsercare",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "lint": "turbo lint",
    "prepare": "is-ci || husky install",
    "typecheck": "turbo typecheck"
  },
  "prettier": "@browsercare/prettier-config",
  "devDependencies": {
    "@browsercare/eslint-config": "workspace:*",
    "@browsercare/prettier-config": "workspace:*",
    "@browsercare/typescript-config": "workspace:*",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "husky": "^9.0.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "turbo": "^1.11.3"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=20"
  }
}
